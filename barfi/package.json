{
  "name": "barfi",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./build/bin/www",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && tscp",
    "build:watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\") && tscp"
  },
  "dependencies": {
    "cloudflare": "^2.9.1",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.3",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pcloud-sdk-js": "^2.0.0",
    "puppeteer": "^19.8.2"
  },
  "devDependencies": {
    "@types/cloudflare": "^2.7.9",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "tsc-alias": "^1.8.5",
    "tscpaths": "^0.0.9",
    "typescript": "^5.0.2",
    "typescript-cp": "^0.1.7"
  }
}
